{
    "name": "rad/rad-framework-builder",
    "description": "RAD Framework Build classes and controllers skeletons from database.",
    "authors": [
        {
            "name": "Guillaume Monet",
            "email": "guillaume.monet@free.fr"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rad\\": "src/Rad/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "rad/rad-framework-builder",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/guillaumemonet/rad-framework-builder.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": {
                        "Rad\\": "src/Rad/"
                    }
                }
            }
        }
    ],
    "require": {
        "php": " ^7.1",
        "rad/rad-framework": "dev-master"
    },
    "require-dev": {
        "phpunit/php-timer": "^1.0."
    },
    "bin": ["bin/rad-builder"]
}
